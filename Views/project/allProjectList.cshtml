@model IEnumerable<ScrumMainApp.Models.Project>

<div style="width:475px; ">

<a class="btn btn-primary btn-mini" href="@Url.Action("Create", "Project")"><i class="icon-plus-sign icon-white"></i> Create Project</a>
<table  class="table table-striped table-bordered table-hover" style="margin-top:10px;">
    <tr>
        <th>
            Projects
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.projectName, "details","project",new { id=item.projectId }, null) 
            
        </td>
        <td>
        <a href="@Url.Action("Edit","Project", new { id = item.projectId }, null)" > <i class="icon-pencil icon-black" title="edit"></i></a> |
            <a href="@Url.Action("Delete","Project", new { id=item.projectId }, null)"> <i class="icon-remove icon-black" title="delete"></i></a> 
        </td>
    </tr>
}

</table>
</div>