@model IEnumerable<ScrumMainApp.Models.Project>

@{
    ViewBag.Title = "Index";
    ViewBag.header = "Projects List";
}


<div class="container">


<div style="width:500px; margin-left:auto; margin-right:auto">


<table  class="table table-striped table-bordered table-hover" style="margin-top:10px;">
    <tr>
        <th>
            Your Projects
        </th>
      
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.projectName, "Dashboard","Project",new { id=item.projectId }, null) 
            
        </td>
       
    </tr>
}

</table>
</div>
</div>
